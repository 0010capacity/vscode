[
	{
		"test": "\\.ts$",
		"jsc": {
			"parser": {
				"syntax": "typescript",
				"tsx": false,
				"decorators": true
			},
			"target": "es2020",
			"loose": false,
			"minify": {
				"compress": false,
				"mangle": false
			}
		},
		"module": {
			"type": "amd",
			"noInterop": true
		},
		"minify": false
	}
]
